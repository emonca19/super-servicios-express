{
  "name": "super-servicios-client",
  "version": "1.0.0",
  "private": true,
  "description": "Static client for Super Servicios (agendar-cita)",
  "scripts": {
    "dev": "live-server --port=3000 --open=agendar-cita.html",
    "build:css": "npx tailwindcss -i ./src/input.css -o ./assets/css/tailwind.css --minify",
    "watch:css": "npx tailwindcss -i ./src/input.css -o ./assets/css/tailwind.css --watch"
  },
  "devDependencies": {
    "live-server": "^1.2.1",
    "tailwindcss": "^3.4.0",
    "postcss": "^8.4.0",
    "autoprefixer": "^10.4.0"
  }
}
